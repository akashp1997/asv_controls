<launch>
    <node name="pid" pkg="pid" type="controller" ns="left_thruster" output="screen" >
      <param name="Kp" value="0.5" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="1900" />
      <param name="lower_limit" value="1100" />
      <param name="windup_limit" value="10" />
      <param name="cutoff_frequency" value="20" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
      <param name="topic_from_controller" value="command" />
      <param name="topic_from_plant" value="odom" />
      <param name="setpoint_topic" value="cmd_vel" />
     </node>

    <node name="pid" pkg="pid" type="controller" ns="right_thruster" output="screen" >
      <param name="Kp" value="0.5" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="1900" />
      <param name="lower_limit" value="1100" />
      <param name="windup_limit" value="10" />
      <param name="cutoff_frequency" value="20" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
      <param name="topic_from_controller" value="command" />
      <param name="topic_from_plant" value="odom" />
      <param name="setpoint_topic" value="cmd_vel" />
     </node>

     <node name="thruster_control_set" pkg="asv_controls" type="control_set.py" />

     <node name="pwm_pub" pkg="asv_controls" type="pwm.py" />
</launch>
